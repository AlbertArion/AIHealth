// 文档教程: https://uniapp.dcloud.net.cn/uniCloud/schema
{
	"bsonType": "object",
	"required": [],
	"permission": {
		"read": true,
		"create": true,
		"update": true,
		"delete": true
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"uid": {
			"bsonType": "string",
			"description": "用户ID"
		},
		"orderId": {
			"bsonType": "string",
			"description": "订单ID"
		},
		"recordId": {
			"bsonType": "string",
			"description": "病历ID"
		},
		"poid": {
			"bsonType": "string",
			"description": "产品订单ID"
		},
		"createDate": {
			"bsonType": "timestamp",
			"description": "创建时间",
			"forceDefaultValue": {
				"$env": "now"
			}
		},
		"totalPrice": {
			"bsonType": "double",
			"description": "支付金额/元"
		},
		"status": {
			"bsonType": "int",
			"description": "支付状态0否1是"
		}
	}
}